{
  "name": "kyles-consulting",
  "version": "1.0.0",
  "description": "a place to register and pay for consulting.",
  "main": "lib/server.js",
  "directories": {
    "lib": "lib"
  },
  "engines": {
    "node": "8.9.4"
  },
  "scripts": {
    "test": "jest --watch",
    "dev": "set NODE_PATH=./lib&& nodemon lib/server.js --exec babel-node --presets stage-2",
    "webpack": "webpack -wd",
    "verify-tests": "jest --coverage",
    "build-webpack": "webpack -p",
    "build-node": "babel lib -d build --copy-files",
    "start-prod": "set NODE_PATH=./build && node ./build/server.js "
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./lib/setupTests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smhood/CollegeMovers.git"
  },
  "author": "Scott Hood",
  "license": "ISC",
  "homepage": "https://github.com/smhood/CollegeMovers#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-env": "^2.4.1",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^3.3.7",
    "css-loader": "^1.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "react": "^16.5.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.5.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^3.0.0",
    "webpack-cli": "^3.1.0"
  },
  "devDependencies": {
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "jest": "^23.6.0"
  }
}
